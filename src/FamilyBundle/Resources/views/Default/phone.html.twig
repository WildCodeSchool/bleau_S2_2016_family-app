    <table class="col s12 m12 l6 offset-l3 bordered highlight centered">
        <thead>
        <tr>
            <th data-field="id">Nom</th>
            <th data-field="name">Prenon</th>
            <th data-field="price">Tel Domicile</th>
            <th data-field="price">Tel Portable</th>
            <th data-field="price">Tel Internet</th>
        </tr>
        </thead>

        <tbody>
        {% for user in phones %}
            <tr>
                <td>{{ user.nom }}</td>
                <td>{{ user.prenom }}</td>
                <td>{{ user.domicile }}</td>
                <td>{{ user.portable }}</td>
                <td>{{ user.box }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
