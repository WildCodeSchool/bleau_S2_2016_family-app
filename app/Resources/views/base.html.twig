    <script type="text/javascript" src="{{ asset('bundles/family/js/materialize.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/family/js/fullcalendar.min.js') }}"></script>

    {#<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>#}
    {#<script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>#}

    {% block js %}
    <script type="text/javascript" src="{{ asset('bundles/family/js/main.js') }}"></script>
    {% endblock %}

    </body>
</html>
