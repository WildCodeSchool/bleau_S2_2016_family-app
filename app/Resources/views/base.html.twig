<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        {#<title>{% block title %}Family App Homepage{% endblock %}</title>#}
        <link rel="icon" type="image/x-icon" href="{{ asset('bundles/family/images/familyicon.ico') }}" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        {% block stylesheet%}
        <!--Import Google Icon Font-->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        {#<link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />#}
        <link href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/fonts/roboto/Roboto-Regular.ttf" />
        <!--Import materialize.css-->
        {#<link type="text/css" rel="stylesheet" href="{{ asset('bundles/family/css/fingerpaint.min.css') }}">#}
        <link type="text/css" rel="stylesheet" href="{{ asset('bundles/family/css/materialize.min.css') }}" media="screen,projection">



	    {% endblock %}
    </head>
    <body>
    <title>{% block title %}{% endblock %}</title>
    {% block header %}{% endblock %}
    {% block body %}{% endblock %}
